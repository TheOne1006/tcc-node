{
  "name": "tcc-node",
  "description": "node 版本的 tcc 事务",
  "version": "1.0.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "doc": "jsdoc -c ./jsdoc.conf.json --readme ./README.md -r -d docs",
    "createGen": "thrift -out ./thriftExport/gen --gen js:node thriftExport/idl/tcc.thrift",
    "dev": "NODE_ENV=devploment WITH_QUEUE=1 WITH_SERVER=1 WITH_SCHEDULE=1 DEBUG=false node index.js",
    "start-pro": "NODE_ENV=production DEBUG=false pm2-runtime start pm2.json",
    "prod-stop": "NODE_ENV=production DEBUG=false pm2 stop index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha",
    "cov": "NODE_ENV=test nyc --reporter=lcov --reporter=html --reporter=text-summary ./node_modules/.bin/mocha",
    "migrate:new": "npx sequelize migration:generate",
    "model:create": "npx sequelize-cli model:generate",
    "migrate:up": "DEBUG='*' npx sequelize db:migrate",
    "migrate:down": "npx sequelize db:migrate:undo",
    "migrate:down:all": "npx sequelize db:migrate:undo:all",
    "migrate:rest": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate",
    "seed:init": "npx sequelize init:seeders",
    "seed:new": "npx sequelize seed:generate",
    "seed:up": "npx sequelize db:seed:all",
    "seed:down": "npx sequelize db:seed:undo",
    "seed:down:all": "npx sequelize db:seed:undo:all",
    "seed:rest": "DEBUG='*' npx sequelize db:seed:undo:all && npx sequelize db:seed:all"
  },
  "dependencies": {
    "bluebird": "^3.7.1",
    "bull": "3.12.1",
    "cache-manager": "^2.10.1",
    "cache-manager-ioredis": "^1.0.1",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "log4js": "^6.0.0",
    "lru-cache": "^5.1.1",
    "mapper-json": "^0.0.3",
    "moment": "^2.24.0",
    "mysql2": "^2.0.0",
    "node-schedule": "^1.3.2",
    "sequelize": "^5.8.6",
    "thrift": "^0.12.0",
    "urllib": "^2.34.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  }
}
